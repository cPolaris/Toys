{
  "manifest_version": 2,
  "name": "Kindly Baidu",
  "description": "Sanitize Baidu search results",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://www.baidu.com/*",
                  "http://baidu.com/*",
                  "https://www.baidu.com/*",
                  "https://baidu.com/*"],
      "js": ["baidu_kindly.js"]
    }
  ]
}
